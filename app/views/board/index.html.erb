<h1>Tablica ogłoszeń inFakt University</h1>
<h2>Lista osób zakwalifikowanych:</h2>
<% @passed.each do |student| %>
<strong><%= student.name %></strong> - ilość punktów: <%= student.score %><br/>
<% end %>
<h2>Lista osób niezakwalifikowanych:</h2>
<% @failed.each do |student| %>
<strong><%= student.name %></strong> - ilość punktów: <%= student.score %><br/>
<% end %>
<br/><br/>
<%= link_to 'Przejdź do admina dla dziekanatu >', students_path %>
